@inherits AppCode.Razor.AppRazor
@using AppCode.Data

<div class="mx-auto max-w-7xl grid grid-cols-1 gap-6 px-2 mt-24 lg:grid-cols-3 lg:gap-8 lg:px-4 lg:mt-32">
  @foreach (var person in AsList<Person>(MyItems))
  {
    <div class="col-span-1" itemscope itemtype="http://schema.org/Person" @Kit.Toolbar.Default(person)>
      @Html.Partial("Part-Properties.cshtml", person)
    </div>
  }
</div>

